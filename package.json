{
  "name": "y",
  "version": "1.0.0",
  "description": "React-GraphQL-Typescript project",
  "main": "index.js",
  "scripts": {
    "watch": "tsc -w",
    "dev": "nodemon dist/index.js",
    "dev2": "nodemon --exec ts-node src/index.ts",
    "start": "node dist/index.js",
    "start2": "ts-node src/index.ts",
    "create:migration" : "mikro-orm migration:create",
    "sql": "pg_ctl -D /usr/local/var/postgres start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jonescamilla/React-GraphQL-TypeScript.git"
  },
  "author": "Jonathan Escamilla",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jonescamilla/React-GraphQL-TypeScript/issues"
  },
  "homepage": "https://github.com/jonescamilla/React-GraphQL-TypeScript#readme",
  "dependencies": {
    "react": "^16.13.1"
  },
  "devDependencies": {
    "@mikro-orm/cli": "^4.0.4",
    "@mikro-orm/core": "^4.0.4",
    "@mikro-orm/migrations": "^4.0.4",
    "@mikro-orm/postgresql": "^4.0.4",
    "@types/express": "^4.17.8",
    "@types/node": "^14.11.1",
    "@types/react": "^16.9.49",
    "apollo-server-express": "^2.18.0",
    "express": "^4.17.1",
    "graphql": "^15.3.0",
    "nodemon": "^2.0.4",
    "pg": "^8.3.3",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^9.0.0",
    "type-graphql": "^1.0.0",
    "typescript": "^4.0.3"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/mikro-orm.config.ts",
      "./dist/mikro-orm.config.js"
    ]
  }
}
